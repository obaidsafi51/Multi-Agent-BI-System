{
  "kimi_api": {
    "base_url": "https://api.moonshot.cn/v1",
    "default_model": "moonshot-v1-8k",
    "timeout": 30,
    "max_retries": 3,
    "retry_delay": 1.0,
    "temperature": 0.1,
    "max_tokens": 2000
  },
  "redis": {
    "url": "redis://localhost:6379",
    "context_ttl": 3600,
    "session_ttl": 1800
  },
  "rabbitmq": {
    "url": "amqp://localhost:5672",
    "exchange": "ai_cfo_agents",
    "routing_keys": {
      "data_agent": "data.query",
      "visualization_agent": "viz.generate",
      "personalization_agent": "personal.learn"
    }
  },
  "query_processing": {
    "max_query_length": 1000,
    "min_confidence_threshold": 0.5,
    "enable_preprocessing": true,
    "enable_ambiguity_detection": true
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}